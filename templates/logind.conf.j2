[Login]
HandleHibernateKey={{ systemd_logind_handlehibernatekey }}
HandleLidSwitch={{ systemd_logind_handlelidswitch }}
HandleLidSwitchDocked={{ systemd_logind_handlelidswitchdocked }}
HandlePowerKey={{ systemd_logind_handlepowerkey }}
HandleSuspendKey={{ systemd_logind_handlesuspendkey }}
HibernateKeyIgnoreInhibited={{ 'yes' if systemd_logind_hibernatekeyignoreinhibited is defined and systemd_logind_hibernatekeyignoreinhibited else 'no' }}
HoldoffTimeoutSec={{ systemd_logind_holdofftimeoutsec }}
IdleAction={{ systemd_logind_idleaction }}
IdleActionSec={{ systemd_logind_idleactionsec }}
InhibitDelayMaxSec={{ systemd_logind_inhibitdelaymaxsec }}
InhibitorsMax={{ systemd_logind_inhibitorsmax }}
KillExcludeUsers={{ systemd_logind_killexcludeusers|join(' ') }}
KillOnlyUsers={{ systemd_logind_killonlyusers|join(' ') }}
KillUserProcesses={{ 'yes' if systemd_logind_killuserprocesses is defined and systemd_logind_killuserprocesses else 'no' }}
LidSwitchIgnoreInhibited={{ 'yes' if systemd_logind_lidswitchignoreinhibited is defined and systemd_logind_lidswitchignoreinhibited else 'no' }}
NAutoVTs={{ systemd_logind_nautovts }}
PowerKeyIgnoreInhibited={{ 'yes' if systemd_logind_powerkeyignoreinhibited is defined and systemd_logind_powerkeyignoreinhibited else 'no' }}
RemoveIPC={{ 'yes' if systemd_logind_removeipc is defined and systemd_logind_removeipc else 'no' }}
ReserveVT={{ systemd_logind_reservevt }}
RuntimeDirectorySize={{ systemd_logind_runtimedirectorysize }}
SessionsMax={{ systemd_logind_sessionsmax }}
SuspendKeyIgnoreInhibited={{ 'yes' if systemd_logind_suspendkeyignoreinhibited is defined and systemd_logind_suspendkeyignoreinhibited else 'no' }}
UserTasksMax={{ systemd_logind_usertasksmax }}
